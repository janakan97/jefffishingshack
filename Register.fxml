<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Register.RegisterController">
   <children>
      <VBox alignment="CENTER" prefHeight="323.0" prefWidth="600.0" AnchorPane.bottomAnchor="77.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextField fx:id="fulname" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="35.0" prefWidth="255.0" promptText="Enter FullName">
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="email" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="35.0" prefWidth="255.0" promptText="Enter Email">
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="pass" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="35.0" prefWidth="255.0" promptText="Enter Password">
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </TextField>
            <TextArea fx:id="address" maxWidth="-Infinity" prefHeight="75.0" prefWidth="255.0" promptText="Enter Address">
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </TextArea>
            <Button fx:id="register" alignment="CENTER" mnemonicParsing="false" onAction="#registerdetail" prefHeight="25.0" prefWidth="130.0" text="REGISTER">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></Button>
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </VBox>
      <Button fx:id="Back" mnemonicParsing="false" onAction="#back" text="BACK" />
   </children>
</AnchorPane>
